# DATES - EXAMPLE FILE
# ===============================
# 01/01/1800 -- 12/31/2030
# ===============================
# -- MM/DD/YYYY --

%alias 31d #[0-31]
%alias 30d #[0-30]

%alias fev 02#[0-29]

%alias 31m [01|03|05|07|08|10|12]$31d;
%alias 30m [04|06|09|11]$30d;

%alias ye #[1800-2030]

$fev;$ye;
$30m;$ye;
$31m;$ye;
