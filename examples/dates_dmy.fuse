# DATES - EXAMPLE FILE
# ===============================
# 01/01/1800 -- 31/12/2030
# ===============================
# -- DD/MM/YYYY --

%alias 31d #[0-31]
%alias 30d #[0-30]

%alias fev #[0-29]02

%alias 31m $31d;[01|03|05|07|08|10|12]
%alias 30m $30d;[04|06|09|11]

%alias ye #[1800-2030]

$fev;$ye;
$30m;$ye;
$31m;$ye;
